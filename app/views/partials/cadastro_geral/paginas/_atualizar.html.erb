
<h1>Partiel atualizar</h1>
<div class="panel-body">
       <form name="pdd" action="atualizado" method="post">
           
           
<%@tela.each do |item|%>
                
                        
                        <div class="form-group">
                <label for="exampleInputPassword1">Nome fantasia</label>
                <input type="text" name="fantname" value="<%=item['nomefantasia']%>" class="form-control"  >
            </div>
                       
                        <div class="form-group">
